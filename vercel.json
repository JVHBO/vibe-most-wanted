{
  "headers": [
    {
      "source": "/.well-known/farcaster.json",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron/daily-reminders",
      "schedule": "0 12 * * *"
    }
  ]
